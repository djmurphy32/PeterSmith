{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "object-literal-shorthand": false,
    "no-console": {
      "severity": "warn"
    }
  }
}
